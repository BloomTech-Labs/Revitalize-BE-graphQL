{
  "name": "revitalize",
  "version": "1.0.0",
  "description": "Debt free tradeschool back-end",
  "main": "server.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "babel src --out-dir dist --copy-files",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "dev": "env-cmd -f ./config/dev.env nodemon src/index.js --ext js,graphql,env --exec babel-node",
    "get-schema": "env-cmd -f ./config/dev.env graphql get-schema -p prisma",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Revitalized-Playground/Back-End.git"
  },
  "author": "Alpha team wolf squadron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Revitalized-Playground/Back-End/issues"
  },
  "homepage": "https://github.com/Revitalized-Playground/Back-End#readme",
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "bcryptjs": "^2.4.3",
    "env-cmd": "^10.0.1",
    "global": "^4.4.0",
    "graphql-cli": "^3.0.14",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter-oauth2": "^2.1.1",
    "prisma-client-lib": "^1.34.8"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^1.19.2"
  }
}
