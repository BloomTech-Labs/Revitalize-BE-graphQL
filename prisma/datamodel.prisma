type User {
  id: ID! @id
  firstName: String
  lastName: String
  email: String! @unique
  password: String
  profileImage: String
  city: String
  zip: String
  address: String
  state: String
  aptNumber: String
  projects: [Project!]
}

type Project {
  id: ID! @id
  user: User!
  name: String!
  description: String!
  address: String!
  state: String!
  zip: String!
  city: String!
  goalAmount: Float!
  amountFunded: Float!
}
